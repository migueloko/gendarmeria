<p id="notice"><%= notice %></p>

<h1>Listing Ficha Carcelaria</h1>

<table>
  <thead>
    <tr>
      <th>Codigo ficha ingreso</th>
      <th>Fecha ingreso date</th>
      <th>Rut reo</th>
      <th>Reo</th>
      <th>Ficha ingreso </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ficha_carcelaria.each do |ficha_carcelarium| %>
      <tr>
        <td><%= ficha_carcelarium.codigo_ficha_ingreso %></td>
        <td><%= ficha_carcelarium.fecha_ingreso_date %></td>
        <td><%= ficha_carcelarium.rut_reo %></td>
        <td><%= ficha_carcelarium.reo %></td>
        <td><%= ficha_carcelarium.ficha_ingreso_ %></td>
        <td><%= link_to 'Show', ficha_carcelarium %></td>
        <td><%= link_to 'Edit', edit_ficha_carcelarium_path(ficha_carcelarium) %></td>
        <td><%= link_to 'Destroy', ficha_carcelarium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

