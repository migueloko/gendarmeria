<p id="notice"><%= notice %></p>

<h1>Listing Beneficios</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Merito a</th>
      <th>Desde</th>
      <th>Hasta</th>
      <th>Reo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @beneficios.each do |beneficio| %>
      <tr>
        <td><%= beneficio.codigo %></td>
        <td><%= beneficio.merito_a %></td>
        <td><%= beneficio.desde %></td>
        <td><%= beneficio.hasta %></td>
        <td><%= beneficio.reo.nombre %></td>
        <td><%= link_to 'Show', beneficio %></td>
        <td><%= link_to 'Edit', edit_beneficio_path(beneficio) %></td>
        <td><%= link_to 'Destroy', beneficio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Beneficio', new_beneficio_path %>
